{
  "name": "mateo-swapps/image-source-manager",
  "description": "Mi plugin para WordPress instalable con Composer",
  "type": "wordpress-plugin",
  "require": {
    "php": ">=7.4",
    "composer/installers": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "mateo-swapps\\ImageSourceManager\\": "src/"
    }
  },
  "extra": {
    "installer-name": "wordpress-plugin"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
